//#include "qore-zmq-module.h"

#include "QC_RouterZSocket.h"

//! The RouterZSocket class implements a ZeroMQ ROUTER socket
/**
 */
qclass RouterZSocket [arg=QoreZSock* sock; ns=Qore::ZMQ; vparent=ZSocket; dom=NETWORK];

//! constructs a PUB zsocket
/** @par Example
    @code
RouterZSocket sock("endpoint");
    @endcode

    @param endpoint
 */
RouterZSocket::constructor(string endpoint) {
   self->setPrivate(CID_ROUTERZSOCKET, new QoreRouterZSock(endpoint->c_str()));
}
